# RiseOra Environment Variables Example
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/riseora

# Supabase Document Vault (Required for Secure Storage)
# Found in Supabase > Project Settings > API
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-secret-service-role-key

# AI Integration (Optional for Dispute Wizard)
AI_INTEGRATIONS_OPENAI_API_KEY=your-openai-api-key
AI_INTEGRATIONS_OPENAI_BASE_URL=https://api.openai.com/v1

# Security
SESSION_SECRET=a-long-random-string-for-sessions
CLIENT_URL=https://riseorra.org
NODE_ENV=development
